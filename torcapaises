#include<stdio.h>
#include<string.h>

void troca_pais(char paisganha[], char paisperde[]){
	char pais;
int i;
	scanf("%c",&pais);
	for(i=0;i<1;i++){

	switch (pais){
	case 'A':
		strcat(paisganha,"A");
		break;
	case 'B':
		strcat(paisganha,"B");
		break;
	case 'C':
		strcat(paisganha,"C");
		break;
	case 'D':
		strcat(paisganha,"D");
		break;
			case 'E':
		strcat(paisganha,"E");
		break;
			case 'F':
		strcat(paisganha,"F");
