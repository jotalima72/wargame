#include <stdio.h>
#include <string.h>
#include <time.h>
#include <stdlib.h>

void printmapa();




void printmapa(){
	int i,j;
		char mapa[15][74] =  {{"**#####                                                                "},
				          {"  #000#******************#####     #####*********##########            "},
				          {"  #####                  #000#     #000#         #000##000# #####******"},
				          {"##### #####             *###############         ##########*#000#      "},
				          {"#000# #000#**************     #000#                   ##### #####      "},
				          {"##### #####                   #####                   #000#    #####   "},
				          {"   #####                         #####                #####    #000#   "},
				          {"   #000#                        *#000#              #####      #####   "},
				          {"   #####                        *#####              #000#         *    "},
				          {"      #####                     *                   #####         *    "},
				          {"      #000#                   ***                    #####        #####"},
				          {"      #####************   #####                      #000#        #000#"},
				          {"                     ######000######*****************#####        #####"},
				          {"                     #000#######000#                     ***********   "},
				          {"                     #####     #####                                   "}};
				      
				      
				      
	for (i=0;i<15;i++){
		printf("\t");
		for (j=0;j<80;j++){
			printf("%c", mapa[i][j]);
		}
		printf("\n");
	}
}
