#include <stdio.h>
#include <string.h>
#include <time.h>
#include <stdlib.h>

void printmapa();




void printmapa(){
	int i,j;
	char mapa[15][74] =  {{"**#####                                                              **"},
				          {"  #000#******************#####     #####*********##########          * "},
				          {"  #-A-#                  #000#     #000#         #000##000# #####***** "},
				          {"##### #####             *#-F-#######-G-#         #-M-##-N-#*#000#      "},
				          {"#000# #000#**************     #000#                   ##### #-O-#      "},
				          {"#-B-# #-C-#                   #-H-#                   #000#    #####   "},
				          {"   #####                         #####                #-P-#    #000#   "},
				          {"   #000#                        *#000#              #####      #-Q-#   "},
				          {"   #-D-#                        *#-I-#              #000#         *    "},
				          {"      #####                     *                   #-R-#         *    "},
				          {"      #000#                   ***                    #####        #####"},
				          {"      #-E-#************   #####                      #000#        #000#"},
				          {"                     ######000######*****************#-S-#        #-T-#"},
				          {"                     #000##-K-##000#                     ***********   "},
				          {"                     #-J-#     #-L-#                                   "}};
				      
				      
				      
	for (i=0;i<15;i++){
		printf("\t");
		for (j=0;j<80;j++){
			printf("%c", mapa[i][j]);
		}
		printf("\n");
	}
}
